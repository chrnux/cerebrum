## $Id$
## Main template
## Gives a 'frames style' left menu and a bottom area
## 

#extends InitTemplate
#implements respond
#from Cerebrum.web.utils import url

#attr $title = "Cereweb"
##
<?xml version='1.0' ?>
<!DOCTYPE HTML PUBLIC
"-//W3C//DTD HTML 4.01//EN"
><html lang="en">
<head>
<title>Cereweb $title</title>
#set css = url("css/fixed.css")
    <link rel="stylesheet" type="text/css" href="$css" />
    <!--[if IE]>
#set css = url("css/fixed_ie5.css")
      <link rel="stylesheet" type="text/css" href="$css" />
    <![endif]-->
#set css = url("css/default.css")
    <link rel="stylesheet" type="text/css" href="$css" />
</head><body>
<div class="content">
<h1>$title</h1>

#def content
Her er mr. Content
#end def content
$content()

</div>


<div class="menu">
#def menu
Dette er menyen <br />
[Viktig] <br />
[Uviktig] <br />
[Hjelp] <br />
#end def menu
$menu()
</div>


<div class="bottom">
#def bottom
Dette er bunnen!!!11
#end def bottom
$bottom()
</div>


</body>
</html>
